<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="40.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.andrewn.java2304javafx.LoginController">
   <children>
      <TextField fx:id="loginField" maxWidth="200.0" prefWidth="200.0" promptText="Логин" styleClass="custom-label" />
      <PasswordField fx:id="passwordField" maxWidth="200.0" prefWidth="200.0" promptText="Пароль" />
      <Button fx:id="loginButton" mnemonicParsing="false" onAction="#login" text="Авторизация">
         <styleClass>
            <String fx:value="custom-button" />
            <String fx:value="custom-button1" />
         </styleClass></Button>
   </children>
</VBox>
